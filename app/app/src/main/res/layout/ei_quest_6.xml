<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/quest"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/material_dynamic_neutral_variant90"
    android:backgroundTint="@color/material_dynamic_secondary95">

    <RelativeLayout
        android:layout_width="640dp"
        android:layout_height="280dp">

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="151dp"
            android:layout_height="300dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            app:srcCompat="@drawable/ei_ipe_amarelo" />

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_alignEnd="@+id/imageView17"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="-485dp"
            android:layout_marginBottom="5dp"
            app:srcCompat="@drawable/ei_abelha" />

        <Button
            android:id="@+id/button"
            android:layout_width="25dp"
            android:layout_height="33dp"
            android:layout_alignParentTop="true"
            android:layout_marginStart="12dp"
            android:layout_marginTop="39dp"
            android:layout_toEndOf="@+id/imageView17"
            android:backgroundTint="#DD3BF6" />

        <Button
            android:id="@+id/button2"
            android:layout_width="25dp"
            android:layout_height="33dp"
            android:layout_alignTop="@+id/imageView18"
            android:layout_marginTop="89dp"
            android:layout_marginEnd="43dp"
            android:layout_toStartOf="@+id/imageView18"
            android:backgroundTint="#002CC1" />

        <Button
            android:id="@+id/button3"
            android:layout_width="25dp"
            android:layout_height="33dp"
            android:layout_alignTop="@+id/imageView18"
            android:layout_marginTop="-16dp"
            android:layout_marginEnd="30dp"
            android:layout_toStartOf="@+id/imageView18"
            android:backgroundTint="#20BD25" />

        <Button
            android:id="@+id/button5"
            android:layout_width="25dp"
            android:layout_height="33dp"
            android:layout_alignTop="@+id/imageView18"
            android:layout_marginTop="-50dp"
            android:layout_marginEnd="-19dp"
            android:layout_toStartOf="@+id/imageView18"
            android:backgroundTint="#DC0F0F" />

        <Button
            android:id="@+id/button4"
            android:layout_width="25dp"
            android:layout_height="33dp"
            android:layout_alignTop="@+id/imageView18"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="46dp"
            android:layout_toStartOf="@+id/imageView18"
            android:backgroundTint="#FFEB1F" />

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignTop="@+id/button"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="-3dp"
            android:layout_toStartOf="@+id/button2"
            android:layout_toEndOf="@+id/imageView17"
            android:visibility="visible"
            app:srcCompat="@drawable/ei_linha01" />

        <ImageView
            android:id="@+id/imageView20"
            android:layout_width="250dp"
            android:layout_height="163dp"
            android:layout_alignTop="@+id/button"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="56dp"
            android:layout_marginTop="-13dp"
            android:layout_marginEnd="-39dp"
            android:layout_marginBottom="92dp"
            android:layout_toStartOf="@+id/button4"
            android:layout_toEndOf="@+id/imageView17"
            android:visibility="visible"
            app:srcCompat="@drawable/ei_linha02" />

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="250dp"
            android:layout_height="200dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="33dp"
            android:layout_marginEnd="0dp"
            android:layout_marginBottom="83dp"
            android:layout_toStartOf="@+id/button3"
            android:layout_toEndOf="@+id/imageView17"
            android:visibility="visible"
            app:srcCompat="@drawable/ei_linha03" />

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="272dp"
            android:layout_height="132dp"
            android:layout_alignTop="@+id/button"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="70dp"
            android:layout_marginTop="-41dp"
            android:layout_marginEnd="-51dp"
            android:layout_marginBottom="151dp"
            android:layout_toStartOf="@+id/button5"
            android:layout_toEndOf="@+id/button"
            android:visibility="visible"
            app:srcCompat="@drawable/ei_linha04" />
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>