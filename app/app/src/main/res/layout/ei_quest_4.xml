<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/quest"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/material_dynamic_neutral30"
    android:backgroundTint="@color/material_dynamic_neutral_variant30">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#A8A8A8"
            android:gravity="center"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/col1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center|center_horizontal"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/fig1"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_margin="2dp"
                    android:background="@drawable/ei_folha_questao_4_folha_1"
                    android:backgroundTintMode="src_in"
                    android:clickable="true"
                    android:tag="f1" />

                <ImageView
                    android:id="@+id/fig2"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_margin="2dp"
                    android:background="@drawable/ei_questao4_folha_1"
                    android:clickable="true"
                    android:tag="f1" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/col2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center|center_horizontal"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/fig3"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_margin="2dp"
                    android:background="@drawable/ei_folha_questao_4_folha_3"
                    android:clickable="true"
                    android:tag="f2" />

                <ImageView
                    android:id="@+id/fig4"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_margin="2dp"
                    android:background="@drawable/ei_questao4_folha_3"
                    android:clickable="true"
                    android:tag="f2" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/col3"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center|center_horizontal"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/fig5"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_margin="2dp"
                    android:background="@drawable/ei_folha_questao_4_folha_2"
                    android:clickable="true"
                    android:tag="f3" />

                <ImageView
                    android:id="@+id/fig6"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_margin="2dp"
                    android:background="@drawable/ei_questao4_folha_2"
                    android:clickable="true"
                    android:tag="f3" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/col4"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center|center_horizontal"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/fig7"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_margin="2dp"
                    android:background="@drawable/ei_folha_questao_4_folha_5"
                    android:clickable="true"
                    android:tag="f4" />

                <ImageView
                    android:id="@+id/fig8"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_margin="2dp"
                    android:background="@drawable/ei_questao4_folha_5"
                    android:clickable="true"
                    android:tag="f4" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/col5"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center|center_horizontal"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/fig9"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_margin="2dp"
                    android:background="@drawable/ei_folha_questao_4_folha_4"
                    android:clickable="true"
                    android:tag="f5" />

                <ImageView
                    android:id="@+id/fig10"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_margin="2dp"
                    android:background="@drawable/ei_questao4_folha_4"
                    android:clickable="true"
                    android:tag="f5" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>